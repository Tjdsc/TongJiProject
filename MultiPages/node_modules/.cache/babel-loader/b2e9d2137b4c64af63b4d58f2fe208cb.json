 default {\r\n\r\n}\r\n</script>\r\n\r\n<style>\r\n/*头部背景*/\r\n.web_top_bg{\r\n    height: 85px;\r\n    background-color: var(--header-background-color);\r\n    z-index: -2;\r\n}\r\n/*头部大盒子*/\r\n.web_top_content{\r\n    width: 1200px;\r\n    margin: 0 auto;\r\n}\r\n.logo{\r\n    float: left;\r\n    margin: 2px 10px 2px 5px;\r\n}\r\n.logo img{\r\n    height: 80px;\r\n    width: 220px;\r\n}\r\n/*动态搜索框模块-start*/\r\n.search-box{\r\n    float: left;\r\n    background: var(--search-background-color);\r\n    height: 60px;\r\n    border-radius: 60px;\r\n    /*padding: 10px;*/\r\n    margin: 10px 20px 10px 50px;\r\n    display: flex;\r\n    justify-content: center;\r\n}\r\n.search-box:hover>.search-txt{\r\n    width: 280px;\r\n    padding: 0 6px;\r\n}\r\n.search-box:hover>.search-btn{\r\n    background: white;\r\n}\r\n.search-btn{\r\n    float: right;\r\n    width: 60px;\r\n    height: 60px;\r\n    border-radius: 50%;\r\n    background: var(--search-background-color);\r\n    display: flex;\r\n    justify-content: center;\r\n    align-items: center;\r\n    transition: 0.4s;\r\n    cursor: pointer;\r\n}\r\n.search-txt{\r\n    border: none;\r\n    background: none;\r\n    outline: none;\r\n    float: left;\r\n    padding: 0;\r\n    color: #ffffff;\r\n    font-size: 16px;\r\n    transition: 0.4s;\r\n    line-height: 60px;\r\n    width: 0;\r\n}\r\n/*动态搜索框模块-end*/\r\n/*头部导航栏_start*/\r\n.nav_container{\r\n    /*position: relative;*/\r\n    float: right;\r\n    height: 100%;\r\n    background-color: var(--search-background-color);\r\n    box-shadow: 0 2px 3px rgba(0, 0, 0, 0.1);\r\n    border-radius: 8px;\r\n}\r\n.nav_container ul{\r\n    display: flex;\r\n    /*justify-content: space-around;*/\r\n    position: relative;\r\n    height: 100%;\r\n}\r\n.nav_container ul > li{\r\n    /*float: left;*/\r\n    height: 85px;\r\n}\r\n.nav_container ul > li > a{\r\n    position: relative;\r\n    /*float: left;*/\r\n    display: block;\r\n    width: 100%;\r\n    height: 100%;\r\n    z-index: 10;\r\n    line-height: 85px;\r\n    font-size: 16px;\r\n    font-weight: bold;\r\n    padding: 0 20px 0 20px;\r\n    color: #ffffff;\r\n}\r\n.nav_container ul > li ol{\r\n    position: relative;\r\n    width: 100%;\r\n    background-color: var(--navigation_background-color);\r\n    border-radius: 8px;\r\n    box-shadow: 1px 1px 3px rgba(0, 0, 0, 0.2);\r\n    /* 让盒子先沿着y轴缩放到0 也就是隐藏了 */\r\n    transform: scaleY(0);\r\n    /* 我们需要将盒子从上面滑动下来 设置一下缩放的中心点即可 设置到最上面的中间位置 */\r\n    transform-origin: 50% 0;\r\n    /* 设置过渡 */\r\n    transition: all 0.5s;\r\n    z-index: 999;\r\n    /*使用z-index该元素必须为除static以外的定位*/\r\n}\r\n.nav_container ul > li ol li{\r\n    height: 70px;\r\n    border-radius: 8px;\r\n}\r\n.nav_container ul > li ol li a{\r\n    display: block;\r\n    color: var(--slideshow-text-color);\r\n    font-size: 14px;\r\n    width: 100%;\r\n    height: 100%;\r\n    line-height: 70px;\r\n    text-align: center;\r\n}\r\n.nav_container ul > li ol li:hover{\r\n    background-color: var(--navigation_background-color);\r\n}\r\n.nav_container ul > li:hover ol{\r\n    transform: scaleY(1);\r\n}\r\n.nav_container ul .background_move {\r\n    position: absolute;\r\n    left: 0;\r\n    top: 0;\r\n    width: 76px;\r\n    height: 100%;\r\n    background-color: var(--navigation_background-color);\r\n    z-index: 9;\r\n    border-radius: 8px;\r\n    transition: all .3s;\r\n}\r\n.start-home,\r\n.nav_container ul > li:nth-child(1):hover~.background_move {\r\n    width: 72px;\r\n    left : 0;\r\n}\r\n.start-management,\r\n.nav_container ul > li:nth-child(2):hover~.background_move {\r\n    width: 104px;\r\n    left : 72px;\r\n}\r\n.start-example,\r\n.nav_container ul > li:nth-child(3):hover~.background_move {\r\n    width: 104px;\r\n    left : 176px;\r\n}\r\n.start-news,\r\n.nav_container ul > li:nth-child(4):hover~.background_move {\r\n    width: 72px;\r\n    left : 280px;\r\n}\r\n.start-about,\r\n.nav_container ul > li:nth-child(5):hover~.background_move {\r\n    width: 104px;\r\n    left : 352px;\r\n}\r\n/*头部导航栏-end*/\r\n</style>"],"sourceRoot":"src/components"}]}