    },],\r\n        animation:true\r\n      },\r\n\r\n      option_all_cell_evaluation : {\r\n        tooltip: {\r\n          position: 'top'\r\n        },\r\n        grid: {\r\n          height: '50%',\r\n          top: '10%'\r\n\r\n        },\r\n        xAxis: {\r\n          name:'电池名称',\r\n          type: 'category',\r\n          data: null,\r\n          axisLabel:{\r\n            interval:0,\r\n            rotate:90,\r\n          },\r\n          splitArea: {\r\n            show: true\r\n          },\r\n          nameLocation:'center',\r\n          nameGap:60,\r\n        },\r\n        yAxis: {\r\n          name:'循环数',\r\n          type: 'category',\r\n          data: null,\r\n          splitArea: {\r\n            show: true\r\n          },\r\n          nameLocation:'center',\r\n          nameGap:40,\r\n        },\r\n        visualMap: {\r\n          min: 0,\r\n          max: 8,\r\n          calculable: true,\r\n          orient: 'horizontal',\r\n          left: 'center',\r\n          bottom: '5%'\r\n        },\r\n        series: [{\r\n          name: 'Punch Card',\r\n          type: 'heatmap',\r\n          data: null,\r\n          label: {\r\n            show: true\r\n          },\r\n          emphasis: {\r\n            itemStyle: {\r\n              shadowBlur: 10,\r\n              shadowColor: 'rgba(0, 0, 0, 0.5)'\r\n            }\r\n          }\r\n        }]\r\n      },\r\n\r\n      option_all_cell_evaluation_explain : {\r\n        tooltip: {\r\n          position: 'top'\r\n        },\r\n        grid: {\r\n          height: '30%',\r\n          width:'30%',\r\n          top: '50%'\r\n        },\r\n        xAxis: {\r\n          name:'跳水程度',\r\n          type: 'category',\r\n          data: ['慢','中','快'],\r\n          axisLabel:{\r\n            interval:0,\r\n            rotate:0,\r\n          },\r\n          splitArea: {\r\n            show: true\r\n          },\r\n          // nameLocation:'center',\r\n          nameGap:20,\r\n        },\r\n        yAxis: {\r\n 